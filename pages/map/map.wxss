/* pages/map/map.wxss */
html,body,page {
  width: 100%;
  height: 100%;
}

view, map {
  width: 100%;
  height: 100%;
}

.page-body {
  background-image: url("data:image/jpeg;base64,")
}

.modal-container-0 {
  border-top: 6px solid #007bff;
}

.modal-container-1 {
  border-top: 6px solid #ffc107;
}

.modal-container-2 {
  border-top: 6px solid #fd7e14;
}

.modal-container-3 {
  border-top: 6px solid #dc3545;
}

.item {
  padding: 1.25rem;
  border: 3px solid #eee;
  border-left-width: .5rem;
  border-radius: .25rem;
}

.modal-head {
  text-align: center
}

.modal-body {
  margin-bottom: 10%;
}

.modal-title {
  display: block
}

.modal-level {
  display: block
}

.modal-desription {
  display: block
}

.modal-body-item {
  margin: 3px 1px;
}

.modal-nav {
  color: #007bff;
}